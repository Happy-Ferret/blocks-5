<?xml version="1.0" encoding="ISO-8859-1"?>
<Dialog>
	<StaticText name="Static1" x="75" y="30">
		<Text>$CE_MENU_FILENAME</Text>
	</StaticText>
	<EditBox name="Filename" x="165" y="30" w="255" h="20">
		<TabStop id="0" />
		<Text></Text>
	</EditBox>
	<Button name="Search" x="425" y="30" w="20" h="20">
		<Title>...</Title>
	</Button>
	<Button name="Load" x="20" y="55" w="135" h="30">
		<Title>$CE_MENU_LOAD</Title>
	</Button>
	<Button name="Save" x="165" y="55" w="135" h="30">
		<Title>$CE_MENU_SAVE</Title>
	</Button>
	<Button name="New" x="310" y="55" w="135" h="30">
		<Title>$CE_MENU_NEW</Title>
	</Button>
	<Button name="Quit" x="455" y="55" w="135" h="30">
		<Title>$CE_MENU_QUIT</Title>
	</Button>

	<Button name="Add" x="280" y="205" w="20" h="20">
		<Title>»</Title>
    <ToolTip>$CE_ADD_LEVEL</ToolTip>
	</Button>
	<Button name="Remove" x="280" y="235" w="20" h="20">
		<Title>«</Title>
    <ToolTip>$CE_REMOVE_LEVEL</ToolTip>
	</Button>
	<Button name="Up" x="570" y="140" w="50" h="20">
		<Title>$CE_LEVEL_UP</Title>
	</Button>
	<Button name="Down" x="570" y="170" w="50" h="20">
